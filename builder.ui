<interface>
	<object id="window" class="GtkWindow">
		<property name="visible">True</property>
		<child>
			<object class="GtkVBox">
				<property name="visible">True</property>
				<child>
					<object class="GtkFrame">
						<property name="label">Score</property>
						<property name="visible">True</property>
						<child>
							<object id="treeview" class="GtkTreeView">
								<property name="visible">True</property>
							</object>
						</child>
					</object>
				</child>
				<child>
					<object class="GtkHButtonBox">
						<property name="visible">True</property>
						<property name="layout-style">GTK_BUTTONBOX_CENTER</property>
						<child>
							<object id="button_nouvelle_donne" class="GtkButton">
								<property name="label">Nouvelle donne</property>
								<property name="visible">True</property>
							</object>
						</child>
						<child>
							<object id="button_nouvelle_partie" class="GtkButton">
								<property name="label">Nouvelle partie</property>
								<property name="visible">True</property>
							</object>
						</child>
						<child>
							<object id="button_quitter" class="GtkButton">
								<property name="label">Quitter</property>
								<property name="visible">True</property>
							</object>
						</child>
					</object>
					<packing>
						<property name="pack_type">GTK_PACK_END</property>
					</packing>
				</child>
			</object>
		</child>
	</object>
</interface>
